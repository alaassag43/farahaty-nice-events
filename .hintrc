{
  "extends": [
    "development"
  ],
  "browserslist": [
    "defaults",
    "not ie 11",
    "not and_ff <= 146",
    "not firefox <= 146",
    "not opera <= 125"
  ],
  "hints": {
    "axe/text-alternatives": [
      "default",
      {
        "image-alt": "off"
      }
    ],
    "axe/name-role-value": [
      "default",
      {
        "button-name": "off"
      }
    ],
    "axe/forms": [
      "default",
      {
        "select-name": "off",
        "label": "off"
      }
    ],
    "no-inline-styles": "off",
    "meta-viewport": "off",
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "user-select",
          "scrollbar-width",
          "backdrop-filter",
          "scrollbar-width: none"
        ]
      }
    ],
    "typescript-config/consistent-casing": "off",
    "typescript-config/strict": "off"
  }
}